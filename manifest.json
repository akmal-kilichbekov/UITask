{
    "_version": "1.12.0",
    "sap.app": {
        "id": "rroot",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "CA-UI5-DOC"
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "rroot.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "rroot.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "rroot.view",
                "controlId": "routingApp",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [{
                    "pattern": "",
                    "name": "main",
                    "target": "main"
                },
                {
                    "pattern": "detail",
                    "name": "detail",
                    "target": "detail"
                }
            ],
            "targets": {
                "main": {
                    "viewId": "main",
                    "viewName": "MainPage"
                },
                "detail": {
                    "viewId": "detail",
                    "viewName": "Orders"
                }
            }
        }
    }
}